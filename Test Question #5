#include <iostream>

strct TreeNode 
{
    int info;
    TreeNode * left, * right;
    TreeNode(int d, TreeNode * lptr, Treenode * rptr)
      : info(d), left(lptr), right(rptr)
    { }
    
};

int sumWithPosParent(TreeNode * root, bool countSelf){
  
  //base case
  if(root == nullptr){
    return 0;
  }

    sum = sumWithPosParent(root->left, root->int >0 );
    sum = sumWithPosParent(root->right, root->int > 0)
  

}
